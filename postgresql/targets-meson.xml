<?xml version="1.0" encoding="UTF-8"?>
<sect3 id="targets-meson-code">
 <title>Cibles de code</title>

 <variablelist>

  <varlistentry id="meson-target-all">
    <term><option>all</option></term>
    <listitem>
     <para>
      Construit tout sauf la documentation
     </para>
    </listitem>
  </varlistentry>

  <varlistentry id="meson-target-backend">
    <term><option>backend</option></term>
    <listitem>
     <para>
      Construit le processus serveur et les modules relatifs
     </para>
    </listitem>
  </varlistentry>

  <varlistentry id="meson-target-bin">
    <term><option>bin</option></term>
    <listitem>
     <para>
      Construit les outils
     </para>
    </listitem>
  </varlistentry>

  <varlistentry id="meson-target-contrib">
    <term><option>contrib</option></term>
    <listitem>
     <para>
      Construit les modules contrib
     </para>
    </listitem>
  </varlistentry>

  <varlistentry id="meson-target-pl">
    <term><option>pl</option></term>
    <listitem>
     <para>
      Construit les langages de procédure
     </para>
    </listitem>
  </varlistentry>

 </variablelist>
</sect3>

<sect3 id="targets-meson-developer">
 <title>Cibles de développeur</title>

 <variablelist>

  <varlistentry id="meson-target-reformat-dat-files">
    <term><option>reformat-dat-files</option></term>
    <listitem>
     <para>
      Reconstruit les fichiers de données du catalogue dans le format standard
     </para>
    </listitem>
  </varlistentry>

  <varlistentry id="meson-target-expand-dat-files">
    <term><option>expand-dat-files</option></term>
    <listitem>
     <para>
      Étend tous les fichiers de données pour inclure les valeurs par défaut
     </para>
    </listitem>
  </varlistentry>

  <varlistentry id="meson-target-update-unicode">
    <term><option>update-unicode</option></term>
    <listitem>
     <para>
      Met à jour les données unicode vers la nouvelle version
     </para>
    </listitem>
  </varlistentry>

 </variablelist>
</sect3>

<sect3 id="targets-meson-documentation">
 <title>Cibles de documentation</title>

 <variablelist>

  <varlistentry id="meson-target-html">
    <term><option>html</option></term>
    <listitem>
     <para>
      Construit la documentation au format HTML multi-page
     </para>
    </listitem>
  </varlistentry>

  <varlistentry id="meson-target-man">
    <term><option>man</option></term>
    <listitem>
     <para>
      Construit la documentation dans le format des pages man
     </para>
    </listitem>
  </varlistentry>

  <varlistentry id="meson-target-docs">
    <term><option>docs</option></term>
    <listitem>
     <para>
      Construit la documentation au format HTML multi-page HTML et au format
      man page
     </para>
    </listitem>
  </varlistentry>

  <varlistentry id="meson-target-doc-src-sgml-postgres-a4.pdf">
    <term><option>doc/src/sgml/postgres-A4.pdf</option></term>
    <listitem>
     <para>
      Construit la documentation au format PDF, avec des pages A4
     </para>
    </listitem>
  </varlistentry>

  <varlistentry id="meson-target-doc-src-sgml-postgres-us.pdf">
    <term><option>doc/src/sgml/postgres-US.pdf</option></term>
    <listitem>
     <para>
      Construit la documentation au format PDF, avec des pages US letter
     </para>
    </listitem>
  </varlistentry>

  <varlistentry id="meson-target-doc-src-sgml-postgres.html">
    <term><option>doc/src/sgml/postgres.html</option></term>
    <listitem>
     <para>
      Contruit la documentation dans un format HTML sur une seule page
     </para>
    </listitem>
  </varlistentry>

  <varlistentry id="meson-target-alldocs">
    <term><option>alldocs</option></term>
    <listitem>
     <para>
      Construit la documentation dans tous les formats supportés
     </para>
    </listitem>
  </varlistentry>

 </variablelist>
</sect3>

<sect3 id="targets-meson-installation">
 <title>Cible d'installation</title>

 <variablelist>

  <varlistentry id="meson-target-install">
    <term><option>install</option></term>
    <listitem>
     <para>
      Installe postgres, mais pas la documentation
     </para>
    </listitem>
  </varlistentry>

  <varlistentry id="meson-target-install-docs">
    <term><option>install-docs</option></term>
    <listitem>
     <para>
      Installe la documentation dans le format HTML multi-page et dans le format man page
     </para>
    </listitem>
  </varlistentry>

  <varlistentry id="meson-target-install-html">
    <term><option>install-html</option></term>
    <listitem>
     <para>
      Installe la documentation dans le format HTML multi-page
     </para>
    </listitem>
  </varlistentry>

  <varlistentry id="meson-target-install-man">
    <term><option>install-man</option></term>
    <listitem>
     <para>
      Installe la documentation dans le format man page
     </para>
    </listitem>
  </varlistentry>

  <varlistentry id="meson-target-install-quiet">
    <term><option>install-quiet</option></term>
    <listitem>
    <para>
      Comme "install", mais les fichiers installés ne sont pas affichés
     </para>
    </listitem>
  </varlistentry>

  <varlistentry id="meson-target-install-world">
    <term><option>install-world</option></term>
    <listitem>
     <para>
      Installe postgres, incluant la documentation en format HTML multi-page,
      et format man page
     </para>
    </listitem>
  </varlistentry>

  <varlistentry id="meson-target-uninstall">
    <term><option>uninstall</option></term>
    <listitem>
     <para>
      Supprime les fichiers installés
     </para>
    </listitem>
  </varlistentry>

 </variablelist>
</sect3>

<sect3 id="targets-meson-other">
 <title>Autres cibles</title>

 <variablelist>

  <varlistentry id="meson-target-clean">
    <term><option>clean</option></term>
    <listitem>
     <para>
      Supprime tous les fichiersde construction
     </para>
    </listitem>
  </varlistentry>

  <varlistentry id="meson-target-test">
    <term><option>test</option></term>
    <listitem>
     <para>
      Exécute tous les tests activés, y compris les modules contrib
     </para>
    </listitem>
  </varlistentry>

  <varlistentry id="meson-target-world">
    <term><option>world</option></term>
    <listitem>
     <para>
      Construit tout, y compris la documentation
     </para>
    </listitem>
  </varlistentry>

  <varlistentry id="meson-target-help">
    <term><option>help</option></term>
    <listitem>
     <para>
      Liste les cibles importantes
     </para>
    </listitem>
  </varlistentry>

 </variablelist>
</sect3>
